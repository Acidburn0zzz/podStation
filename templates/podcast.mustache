<div id="podcast_{{url}}" class="podcastEntry">
	<table>
		<tr>
			<td>
				<img src="{{image}}" class="podcastImage" height="100" width="100"></img>
			</td>
			<td>
				<div class="itemMenu">
					<a href="#" class="removePodcast" id="{{url}}">Remove</a> |
					<a href="#" class="updatePodcast" id="{{url}}">Update</a>
				</div>
				<div class="textView">
					<a href="{{link}}"><h2 class="podcastTitle">{{title}}</h2></a>
					<a href="#Episodes" class=linkToEpisodes id="{{url}}">
						<h3 class="podcastTitle">{{episodesNumber}} Episode(s)</h3>
					</a>
					<h4 class="podcastTitle">Status: {{status}}</h4>
					<p>{{description}}</p>
				</div>
			</td>
		</tr>
	</table>
</div>
