{
	"name": "podStation",
	"description": "A podcast aggregator for Chrome",
	"version": "0.0.1",
	"permissions": [
		"tabs",
		"storage",
		"http://*/",
		"https://*/"
	],
	"browser_action": {
		"default_title": "podStation"
	},
	"background": {
		"scripts": [
			"entities/podcast.js",
			"entities/podcastManager.js",
			"podstation_bg.js"
		],
		"persistent": false
	},
	"options_page": "options.html",
	"manifest_version": 2
}
